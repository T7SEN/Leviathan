{
  "name": "leviathan",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22.12.0"
  },
  "scripts": {
    "build": "tsc -p .",
    "start": "node dist/index.js",
    "deploy:commands": "tsx scripts/deploy-commands.ts",
    "dev:bot": "tsx watch src/index.ts",
    "dev:deploy": "tsx scripts/watch-deploy.ts",
    "dev": "concurrently -k -n bot,deploy \"npm:dev:bot\" \"npm:dev:deploy\"",
    "lint": "eslint .",
    "typecheck": "npx tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "better-sqlite3": "^12.2.0",
    "discord.js": "14.21.0",
    "dotenv": "16.4.5"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^22.17.2",
    "@vitest/coverage-v8": "^3.2.4",
    "chokidar": "^4.0.3",
    "concurrently": "^9.2.0",
    "eslint": "^9.10.0",
    "fast-check": "^4.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5.5.0",
    "vitest": "^3.2.4"
  }
}
